{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://chatofnahrain.runasp.net/api/:path*"
    },
    {
      "source": "/chatHub/:path*",
      "destination": "http://chatofnahrain.runasp.net/chatHub/:path*"
    },
     {
      "source": "/:path*",
      "destination": "http://chatofnahrain.runasp.net/"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}